@import url('https://fonts.googleapis.com/css2?family=Barlow+Semi+Condensed:wght@400&family=Zen+Kaku+Gothic+Antique:wght@300;400;500;700;900&display=swap');

body,html{
    background: #FFF;
    position: relative;
    font-family: 'Barlow Semi Condensed', sans-serif;
    font-family: 'Zen Kaku Gothic New', sans-serif;
}

/*------------------------------------------------

common

-------------------------------------------------*/
div#wrap{
    width:100%;
    height:auto;
    min-height:calc(100% - 40px);
    border-top:20px solid #FC0;
    border-bottom:20px solid #FC0;
}
.content{
    display: block;
    border-left:20px solid #FC0;
    border-right:20px solid #FC0;
    position:relative;
    width:100%;
    height:auto;
}
.content.innerpage_content{
    padding-top:10vw;
}
.content.add_spacer{
    padding-top:5vw;
}

/*-----------header-----------*/
header{
    position:relative;
    display:block;
    width: 100%;
    z-index: 99;
    text-align: right;
}
.fixed{
    position: fixed;
    top: 0;
    right: 0;
}
.absolute{
    position: absolute;
    top: 0;
    right: 0;
}
header.fixed{
    background: rgba(255,255,255,0.9);
    border-bottom:1px solid #000;
    padding:1vw 0 1vw;
}
header.absolute/*下層*/{
    background: none;
    padding:3vw 0;
}

header#menu_index.absolute/*top*/{
    background: none;
    padding:2vw 0;
}
    header #logo_box{
        position: relative;
    }
    header#menu_index.fixed #logo_box,/*top*/
    header#menu.fixed #logo_box,/*下層*/
    header#menu.absolute #logo_box/*下層*/{
        display: block;
    }
    header#menu_index.absolute #logo_box/*top*/{
        display: none;
    }
    #menu_logo{
        position: absolute;
        left:0;
        top:0;
    }
    header #menu_logo{
        width:15vw;
        margin:-0.7vw 0 0 2vw;
        float:left;
    }
    header ul{
        display: inline-block;
    }
    
    header ul li{
        font-size:1.1vw;
        display:inline-block;
        margin-right:2vw;
        vertical-align: middle;
    }
    header ul li:last-child{
        display:inline-block;
        margin-right:0;
    }
    header ul li .menu_cart_btn{
        cursor:pointer;
        position:relative;
        display:block;
        width:13vw;
        height:3.4vw;
        border-left:1px solid #000;
        border-top:1px solid #000;
        border-bottom:1px solid #000;
        background:#FC0;
        border-radius: 9999px 0 0 9999px;
    }
        .menu_cart_btn span.info{
            display:table-cell;
            width:calc(11vw - 2px);
            height:calc(3.4vw - 2px);
            vertical-align: middle;
            text-align: right;
            font-size: 1.1vw;
        }
        .menu_cart_btn .icon{
            display:block;
            position: absolute;
            top:0.3vw;
            left:0.3vw;
            width:calc(2.8vw - 2px);
            height:calc(2.8vw - 2px);
            background: #FFF;
            border-radius: 9999px;
        }
            .menu_cart_btn .icon.cart_look{
                background-image: url("{{ '_img_cart_look.svg' | asset_url }}");
                background-size: 90%;
                background-repeat: no-repeat;
                background-position: 50% 50%;
            }

header #draw_product_menu_box{/*product用の購入boxで、位置だけを参照*/
    width:0;
    height:0;
    position: relative;
    overflow: visible;
    z-index: -1;
}

header.absolute #draw_product_menu_box{
    display:none;
    visibility: hidden;
}
header.fixed #draw_product_menu_box{
    display: block;
    visibility: visible;
}
    #draw_product_menu_box #draw_product_menu{
        position: absolute;
        top:calc(1vw + 1px);left:3vw;
        width:22vw;
        height:4vw;
        overflow: hidden;
        transition : height 0.8s;
        display:block;
    }
    #draw_product_menu_box.active #draw_product_menu{
        height:50vw;
        display:block;
    }
        #draw_product_menu_box .draw_product_content_box{
            transform : translate(0, -31vw);
            transition : transform 0.8s;
        }
        #draw_product_menu_box.active .draw_product_content_box{
            transform : translate(0, 0vw);
        }
        #draw_product_menu_box .draw_product_content{
            opacity : 0;
            margin:1vw 0 0;
            width:22vw;
            min-height:30vw;
            background: #FFF;
            border:1px solid #000;
            border-radius:1vw;
            padding:1vw;
            transition : opacity 0.8s;
        }
        #draw_product_menu_box.active .draw_product_content{
            opacity : 1;
        }
        #draw_product_menu_box .draw_product_content .img{
            display:block;
            width:100%;
            height:11.5vw;
            margin:auto;
            border-radius: 0.5vw;
            background-repeat:no-repeat;
            background-position:center;
            background-size:100% auto;
        }
        #draw_product_menu_box .item_name{
            text-align:left;
        }
            #draw_product_menu_box .item_name h1{
                display: block;
                font-size: 1.3vw;
                line-height: 1.8vw;
                padding:1vw 0 0 0;
            }
            #draw_product_menu_box .item_name span{
                display: block;
                font-size: 1vw;
            }

            #draw_product_menu_box .count{
                font-size:1vw;
                padding:0.3vw 0;
            }
        #draw_product_menu .draw_product_btn{
            width:22vw;
            height:2.5vw;
            padding:0.5vw;
            text-align: center;
            font-size:1.1vw;
            line-height: 1.5vw;
            background: #FC0;
            border:1px solid #000;
            border-radius: 0.5vw;
            margin:0.5vw 0 0 0;
            cursor: pointer;

            transform : translate(0, -31vw);
            transition : transform 0.8s;
        }
        #draw_product_menu_box.active .draw_product_btn{
            transform : translate(0, 0vw);
        }
            #draw_product_menu_box .draw_product_btn .down,
            #draw_product_menu_box.active .draw_product_btn .up{
                display:inline-block;
            }
            #draw_product_menu_box .draw_product_btn .up,
            #draw_product_menu_box.active .draw_product_btn .down{
                display:none;
            }
/*-----------footer-----------*/
footer{
    color:#FFF;
    background: #000;
    padding-bottom:2vw;
}
footer nav ul#footer_nav {
    width:85%;
    margin:auto;
}
    ul#footer_nav li#footer_menu{
        width:70%;
    }
        li#footer_menu ul{
            width:100%;
        }
            li#footer_menu ul li{
                font-size: 1.1vw;
                display: inline-block;
                padding:2vw 0;
                margin-right:2vw;
            }

    ul#footer_nav li#footer_social{
        width:30%;
    }
ul#footer_guide{
    display: flex;
    width:85%;
    margin:auto;
}
    ul#footer_guide li{
        width:32%;
    }
    ul#footer_guide li:nth-child(1),
    ul#footer_guide li:nth-child(2){
        margin-right:2%;
    }

        ul#footer_guide li h5{
            display: block;
            background: #111;
            padding:0.2vw 0.3vw;
            font-size: 1vw;
        }
        ul#footer_guide li span{
            display: block;
            padding:0.5vw 0 0;
            font-size:0.9vw;
            line-height: 1.5vw;
        }
ul#footer_support_menu{
    width:85%;
    margin:2vw auto 0;
}
    ul#footer_support_menu li{
        display: inline-block;
        width:auto;
        font-size:1.1vw;
        padding:0.8vw 1.5vw;
    }
    ul#footer_support_menu li:nth-child(1){
        border:1px solid #FFF;
    }
    ul#footer_support_menu li:last-child{/*copyright*/
        float:right;
        text-align: right;
        font-size: 0.8vw;
    }

#information .count{
    font-size:1vw;
    padding:1vw 0;
}
    .count button.plus,
    .count button.minus,
    .count input.number{
        height:2.5vw;
    }
    .count button.plus,
    .count button.minus{
        width:2.5vw;
        border-top:1px solid #333;
        border-bottom:1px solid #333;
        cursor: pointer;
        vertical-align : top;
    }
    .count button.plus{
        border-right:1px solid #333;
    }
    .count button.minus{
        border-left:1px solid #333;
    }
    .count input.number{
        width:3vw;
        border:1px solid #333;
    }
