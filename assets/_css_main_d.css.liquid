/*---------------slider-----------------*/
div#slider_box{
    width:100%;
    background:#FC0;
    position:relative;
}
div#slider_box::before{
    content:"";
    position:absolute;
    left:0;top:1.5vw;
    width:100%;
    height:2.5vw;
    background:#FFF;
}
ul#slider{
    width:100%;
    border-bottom:20px solid #FC0;
}
    ul#slider li.slide_clip{
        position:relative;
        height:20.8vw;

    }
    ul#slider li.slide_clip::after{
        content:"";
        position:absolute;
        left:0;top:0;
        background:url("{{ '_img_slider_chichi.png' | asset_url }}") 0 0 repeat-x;
        background-position:-0.75vw 0;
        background-size: contain;
        width:100%;
        height:14%;
    }
        ul#slider li.slide_clip a{
            display:block;
            position:relative;
            padding:2.4vw 1vw 1vw;
        }

        ul#slider li.slide_clip div.noren{
            position:absolute;
            top:1.5vw;left:0;
            width: 100%;
            height: 100%;
        }

/*---------------ippin_list-----------------*/
.ttl_index_01{
    width:calc(44vw - 20px);
    position: absolute;
    margin:0 0 0 5vw;
}
    .ttl_index_01 h2.read{
        display:block;
        float:left;
        font-size:2vw;
        padding-right:1vw;
        letter-spacing: 0.05vw;
    }
    .ttl_index_01 h2.read span{
        display:block;
        font-size:0.8vw;
    }
    .ttl_index_01 p.description{
        display:block;
        float:left;
        padding-left:1vw;
        font-size:1.3vw;
        border-left:1px solid #000;
    }
ul#ippin{
    width:calc(90vw - 40px);
    margin:0 auto 0;
}

    ul#ippin li.store{
        position: relative;
        display:inline-block;
        width:calc(44vw - 20px);
        height:32vw;
    }
    ul#ippin li.store:nth-child(even){
        float:left;
    }
    ul#ippin li.store:nth-child(odd){
        float:right;
    }
    ul#ippin li.store:nth-child(2){;
        margin-top:15vw;
    }
        ul#ippin li.store div.chef{
            float:left;
            width:57%;
            height:24vw;
            background-repeat:no-repeat;
            background-position:center;
            background-size:100% auto;
            border-radius:1vw;
        }
        div.ribbon_box{
            position: relative;
            width:2.5vw;
            margin:-4% 0 0 5%;
            padding-bottom:1.25vw;
            border-top:1px solid #000;
            border-left:1px solid #000;
            border-right:1px solid #000;
            overflow:hidden;
        }
            div.ribbon_box ul.ribbon_in_img,
            div.ribbon_box ul.ribbon{
                width:100%;
                background: #f5c520;
                padding-top:0.5vw;
                padding-bottom:1.25vw;
                text-align: left;
            }
                div.ribbon_box ul.ribbon li{
                    width:100%;
                    min-height:10vw;
                    -ms-writing-mode: tb-rl;
                    writing-mode: vertical-rl;
                    font-size:1.6vw;
                    line-height: 2.45vw;
                    letter-spacing: 0.1vw;
                }
                div.ribbon_box ul.ribbon_in_img li{
                    width:100%;
                    min-height:10vw;
                }
                    div.ribbon_box ul.ribbon_in_img li img{
                        width:100%;
                    }
            div.ribbon_box ul.ribbon_in_img::before,
            div.ribbon_box ul.ribbon::before{
                content: "";
                display: block;
                left:-0.925vw;
                bottom:0.35vw;
                position: absolute;
                background:#f5c520;
                width:1.767vw;
                height:1.767vw;
                -webkit-transform: rotate(-45deg);
                transform: rotate(-45deg);
                border-bottom:1px solid #000;
            }
            div.ribbon_box ul.ribbon_in_img::after,
            div.ribbon_box ul.ribbon::after{
                content: "";
                display: block;
                right:-0.925vw;
                bottom:0.35vw;
                position: absolute;
                background:#f5c520;
                width:1.767vw;
                height:1.767vw;
                -webkit-transform: rotate(-45deg);
                transform: rotate(-45deg);
                border-left:1px solid #000;
            }

        ul#ippin li.store div.ippin{
            float:right;
            width:43%;
            height:25vw;
            background-repeat:no-repeat;
            background-position:center;
            background-size:100% auto;
            border-radius:1vw;
        }
        ul#ippin li.store div.ippin_img{
            width:85%;
            height:62%;
            margin:-4% auto 0;
        }
            .mask {
                -webkit-clip-path: url(#svgPath);
                clip-path: url(#svgPath);
            }
        ul#ippin li.store div.ippin_text{
            display: table;
            width:100%;
            height:30%;
        }
            div.ippin_text div.ippin_text_rayout{
                display: table-cell;
                width:100%;
                height:100%;
                text-align: center;
                vertical-align: middle;
            }
                div.ippin_text_rayout span{
                    font-size:0.9vw;
                    line-height:1vw;
                    padding:0.2vw 0.5vw;
                }
                div.ippin_text_rayout p{
                    display:block;
                    padding:0.2vw 0.5vw;
                    font-size:1.5vw;
                    line-height:2vw;
                    text-decoration: underline #000 1px;
                    text-underline-offset: 0.25vw;
                }

/*-----------concept-----------*/
div#concept #concept_box {
    width:100%;
    border-top:1px solid #000;
}
    div#concept #concept_box .concept_wrap{
        width: 100%;
        display:flex;
        justify-content: center;
    }
        .concept_wrap .concept_h{
            position: relative;
            width:19%;
            -ms-writing-mode: tb-rl;
            writing-mode: vertical-rl;
        }
           .concept_h h1{
                display: inline;
                position: absolute;
                writing-mode: vertical-rl;
                -ms-writing-mode: tb-rl;
                left: 50%;
                top: 50%;
                transform: translate(-50%, -50%);
                font-size:1.8vw;
                line-height: 3vw;
                letter-spacing: 0.2vw;
                white-space:nowrap;
            }
        .concept_wrap #mov_box{
            width:48%;
            padding:1vw 0;
        }
            #mov_box #mov{
                position: relative;
                    width:100%;
                    height: 27vw;
            }
                #mov #yt_player{
                    top:0;left:0;
                    z-index: 1;
                    width:100%;
                    height: 100%;
                    aspect-ratio: 16 / 9;
                }
                #yt_overlay{
                    position: absolute;
                    top:0; left:0;
                    z-index: 2;
                    width:100%;
                    height:100%;
                    background: #000 url("{{ '_img_yt_overlay.svg' | asset_url }}") no-repeat center center;
                    background-size: 80%;
                    opacity: 0.3;
                }
        .concept_wrap .concept_t{
            position: relative;
            width:33%;
            -ms-writing-mode: tb-rl;
            writing-mode: vertical-rl;
        }
            .concept_t p{
                display: inline;
                position: absolute;
                writing-mode: vertical-rl;
                -ms-writing-mode: tb-rl;
                left: 50%;
                top: 50%;
                transform: translate(-50%, -50%);
                font-size:1vw;
                line-height: 1.7vw;
                letter-spacing: 0.05vw;
                white-space:nowrap;
            }
