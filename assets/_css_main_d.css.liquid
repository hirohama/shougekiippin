.btn_box{/*indexのみ*/
    position: absolute;
    left:calc(13vw - 10px);
    top:22.3vw;
}

.btn{
    cursor:pointer;
    position:relative;
    display:block;
    width:18vw;
    height:3.4vw;
    border:1px solid #000;
    background:#FFF;
    text-align: center;
    border-radius: 9999px;
    margin:auto;
}
    .btn span.info{
        display:table-cell;
        width:calc(15vw - 2px);
        height:calc(3.4vw - 2px);
        vertical-align: middle;
        font-size: 1.2vw;
    }
        /*.btn span.info */span.quantity_str{
            font-size:1vw;
        }
        /*.btn span.info */span.quantity_str::after{
            content:'\0a5';
            letter-spacing: -0.3vw;
            font-size:1vw;
        }
        /*.btn span.info */span.price_str{
            font-size:1.5vw;
        }
        /*.btn span.info */span.tax_str{
            font-size:1vw;
        }
        /*.btn span.info */span.cart_count{
            font-size:0.8vw;
        }
    .btn .icon{
        display:block;
        position: absolute;
        top:0.3vw;
        right:0.3vw;
        width:calc(2.8vw - 2px);
        height:calc(2.8vw - 2px);
        background: #FC0;
        border-radius: 9999px;
        background-size: 90%;
        background-repeat: no-repeat;
        background-position: 50% 50%;
    }
